@{
    ViewData["Title"] = "TimeLogSummaryReport";
}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h3 class="text-primary">ProjectTime Time Log Summary Report</h3>
            <h5 class="text-primary">Use the Search bar to filter report</h5>
        </div>
        <div class="col-6 text-end">
           <a asp-area="User" asp-controller="Home" asp-action="Index" class="btn btn-secondary">
               <i class="bi bi-skip-backward"></i> &nbsp; Home Page
           </a>
        </div>
   </div>
  <div id="printbar" style="float:right"></div>
<table id="tblDatatimelogsummaryreport" class="table table-bordered table-striped" style`="width 100%">  
    <thead> 
        <tr>
            <th>Project Code</th>
            <th>Project</th>
            <th>Log Start Date</th>
            <th>Log End Date</th>
            <th>Total Duration Days Logged</th>
            <th>Project Description</th>
        </tr>
    </thead>
</table>
</div> 
@section scripts{
    <script src="~/js/timelogsummaryreport.js"></script>

}