@{
    ViewData["Title"] = "RescourcingReport";

}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h3 class="text-primary">ProjectTime Rescourcing Report</h3>
            <h5 class="text-primary">Use the Search bar to filter report</h5>
        </div>
        <div class="col-6 text-end">
           <a asp-area="User" asp-controller="Home" asp-action="Index" class="btn btn-secondary">
               <i class="bi bi-skip-backward"></i> &nbsp; Home Page
           </a>
        </div>
   </div>
 <div id="printbar" style="float:right"></div>
<table id="tblDatarescourcingreport" class="table table-bordered table-striped" style`="width 100%">  
    <thead> 
        <tr>
            <th>Project</th>
            <th>Department</th>
            <th>Est.Start Date</th>
            <th>Est.End Date</th>
            <th>Est.Duration Days</th>
            <th>Description</th>
        </tr>
    </thead>
</table>
</div> 
@section scripts{

    <script src="~/js/rescourcingreport.js"></script>
}
