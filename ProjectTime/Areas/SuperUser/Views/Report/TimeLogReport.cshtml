
@{
    ViewData["Title"] = "TimeLogReport";

}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h3 class="text-primary">ProjectTime Time Log Report (All)</h3>
            <h5 class="text-primary">Input Min & Max dates or use the Serach bar to filter report</h5>
        </div>
        <div class="col-6 text-end">
           <a asp-area="User" asp-controller="Home" asp-action="Index" class="btn btn-secondary">
               <i class="bi bi-skip-backward"></i> &nbsp; Home Page
           </a>
        </div>
   </div>
 <table border="0" cellspacing="5" cellpadding="5">
        <tbody><tr>
            <td>Minimum date:</td>
            <td><input type="text" id="min" name="min"></td>
        </tr>
        <tr>
            <td>Maximum date:</td>
            <td><input type="text" id="max" name="max"></td>
        </tr>
  </tbody></table>
  <div id="printbar" style="float:right"></div>
<table id="tblDatatimelogreport" class="table table-bordered table-striped" style`="width 100%">  
    <thead> 
        <tr>
            <th>Project</th>
            <th>Department</th>
            <th>Logged By</th>
            <th>Log Date</th>
            <th>Hours Logged</th>
            <th>Log Description</th>
        </tr>
    </thead>
</table>
</div> 
@section scripts{
    <script src="~/js/timelogreport.js"></script>

}