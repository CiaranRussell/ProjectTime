@model IEnumerable<TimeLog>
@{
    ViewData["Title"] = "Index";

}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">My ProjectTime Log Summary</h2>
        </div>
        <div class="col-6 text-end">
           <a asp-controller="TimeLog" asp-action="Create" class="btn btn-primary">
               <i class="bi bi-plus-circle"></i> &nbsp; Log ProjectTime
           </a>
        </div>
   </div>
   <br />
<table id="tblDataindextimelog" class="table table-bordered table-striped" style`="width 100%">  
    <thead> 
        <tr>
            <th>Project Code</th>
            <th>Project</th>
            <th>Log Start Date</th>
            <th>Log End Date</th>
            <th>Total Duration Days Logged</th>
            <th>Itemised Time Log's</th>
        </tr>
    </thead>
</table>
</div> 
@section scripts{
    <script src="~/js/indextimelog.js"></script>
    <script src="//cdn.jsdelivr.net/npm/alertifyjs@1.11.2/build/alertify.min.js"></script>
    <script type="text/javascript">
        $(function(){
            var save = '@TempData["save"]'
            if(save!='') {
                alertify.success(save);
            }
            var edit = '@TempData["edit"]'
            if(edit!='') {
                alertify.success(edit);
            }
            var del = '@TempData["delete"]'
            if (del!='') {
                alertify.error(del);
            }
        })
    </script>
}



